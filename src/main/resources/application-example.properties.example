spring.datasource.url=jdbc:mysql://localhost:3306/YOUR_DATABASE
spring.datasource.username=USERNAME
spring.datasource.password=PASSWORD

spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQLDialect
spring.jpa.hibernate.ddl-auto=update

spring.jpa.properties.hibernate.id.new_generator_mappings=false

#JWT Sceret
zakat.jwt.secret=YOUR_JWT_SECRET

doku.api.client-id=YOUR_DOKU_CLIENT_ID
doku.api.secret-key=YOUR_DOKU_SECRET_KEY

#Xendit key
xendit.api-key=YOUR_XENDIT_SECREAT_KEY
xendit.valid-token=YOUR_XENDIT_WEBHOOK_TOKEN

#smtp gmail
email.username=YOUR_HOST_EMAIL
#using app password for gmail
email.password=YOUR_PASSWORD